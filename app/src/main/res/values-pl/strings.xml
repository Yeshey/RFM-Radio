	<string name="pref_recording_name_title">Nazwa pliku</string>
	<string name="pref_recording_name_value" translatable="false">FM-$h$i$s-$f</string>
	<string name="pref_recording_path_name_description" tools:ignore="TypographyDashes">Dostępne zmienne:\n$y - rok\n$m - miesiąc\n$d - dzień miesiąca\n$h - godzina\n$i - minuta\n$s - sekunda\n$f - częstotliwość (np., 103.4 MHz = 1034)\n$r - losowy numer 10000-99999\nPrzykład:\nKatalog: FM/$y-$m-$d -> FM/2020-01-27\nNazwa Pliku: Rec-$h_$i_$s_$f -> Rec_23_01_49_0906.wav</string>
	<string name="pref_recording_format_title">Format Nagrywania</string>
